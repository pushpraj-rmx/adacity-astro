---
const { pageTitle } = Astro.props;
---

<!-- BREADCRUMB SECTION START -->
<section class="jo-breadcrumb">
  <div class="container">
    <h1 class="jo-page-title jo-section-title">{pageTitle}</h1>
    <ul class="jo-breadcrumb-nav">
      <li><a href="#">Home</a></li>
      <li><span>/</span></li>
      <li class="current-page">{pageTitle}</li>
    </ul>

    <div class="jo-circle-box">
      <span class="circle-1"
        ><img src="assets/img/social-icon-1.png" alt="Social Media Icon" /></span
      >
      <span class="circle-2"
        ><img src="assets/img/social-icon-2.png" alt="Social Media Icon" /></span
      >
      <span class="circle-3"
        ><img src="assets/img/social-icon-3.png" alt="Social Media Icon" /></span
      >
      <span class="circle-4"
        ><img src="assets/img/social-icon-4.png" alt="Social Media Icon" /></span
      >
    </div>
  </div>
</section>
<!-- BREADCRUMB SECTION END -->
